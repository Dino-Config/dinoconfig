{
    "name": "dinoconfig-java-sdk-demo",
    "root": "apps/demo/dinoconfig-java-sdk-demo",
    "sourceRoot": "apps/demo/dinoconfig-java-sdk-demo/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "./gradlew build",
                "cwd": "apps/demo/dinoconfig-java-sdk-demo"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "./gradlew run",
                "cwd": "apps/demo/dinoconfig-java-sdk-demo"
            }
        }
    },
    "tags": ["type:java", "scope:demo"]
}
  