{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs/src",
  "tags": ["type:docs", "scope:public"],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run start",
        "cwd": "apps/docs"
      },
      "continuous": true
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "command": "npm run build",
        "cwd": "apps/docs"
      },
      "cache": true
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run serve",
        "cwd": "apps/docs"
      },
      "continuous": true,
      "dependsOn": ["build"]
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run clear",
        "cwd": "apps/docs"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run deploy",
        "cwd": "apps/docs"
      },
      "dependsOn": ["build"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run typecheck",
        "cwd": "apps/docs"
      },
      "cache": true
    }
  }
}
