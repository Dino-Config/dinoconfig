{
  "name": "dinoconfig-docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/dinoconfig-docs/src",
  "tags": ["type:docs", "scope:public"],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx docusaurus start",
        "cwd": "apps/dinoconfig-docs"
      },
      "continuous": true
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "command": "npx docusaurus build",
        "cwd": "apps/dinoconfig-docs"
      },
      "cache": true
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx docusaurus serve",
        "cwd": "apps/dinoconfig-docs"
      },
      "continuous": true,
      "dependsOn": ["build"]
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx docusaurus clear",
        "cwd": "apps/dinoconfig-docs"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx docusaurus deploy",
        "cwd": "apps/dinoconfig-docs"
      },
      "dependsOn": ["build"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx tsc --noEmit",
        "cwd": "apps/dinoconfig-docs"
      },
      "cache": true
    }
  }
}
