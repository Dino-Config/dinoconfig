{
  "name": "dinoconfig-fe",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/dinoconfig-fe/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/dinoconfig-fe",
        "browser": "apps/dinoconfig-fe/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/dinoconfig-fe/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/dinoconfig-fe/public"
          },
          "apps/dinoconfig-fe/src/_redirects"
        ],
        "styles": [
          "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/dinoconfig-fe/src/styles.scss"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1000kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "16kb",
              "maximumError": "32kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "apps/dinoconfig-fe/src/environments/environment.ts",
              "with": "apps/dinoconfig-fe/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "dinoconfig-fe:build:production"
        },
        "development": {
          "buildTarget": "dinoconfig-fe:build:development"
        }
      },
      "defaultConfiguration": "development",
      "dependsOn": [
        "dinoconfig-be:serve"
      ],
      "options": {
        "proxyConfig": "apps/dinoconfig-fe/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "dinoconfig-fe:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/apps/dinoconfig-fe"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "dinoconfig-fe:build",
        "staticFilePath": "dist/apps/dinoconfig-fe/browser",
        "spa": true
      }
    }
  }
}
